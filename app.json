{
  "expo": {
    "name": "chattingApp",
    "slug": "chattingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dgsw.chattingApp",
      "googleServicesFile":"./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.dgsw.chattingApp",
      "googleServicesFile": "./google-services.json"
    }
  }
}
